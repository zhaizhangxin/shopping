/* pages/winningRecord/winningRecord.wxss */
page{
  background: #f5f5f5;
}
.content{
  background: #f5f5f5;  
}
.contents{
  margin: 0 30rpx;
}

/* 列表 */
.contColls{
  padding: 0 30rpx 30rpx 30rpx;
}
.contColl{
  position: relative;
  margin-top: 30rpx;
  padding: 30rpx;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
}
.collSponsor{
  position: absolute;
  top: 0;
  /* padding: 14rpx 50rpx; */
  width: 300rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color: #fff;
}
.collSponsor image{
  width: 100%;
  height: 100%;
}
.sponsorText{
  position: absolute;
  top: 0;
  width: 100%;
  text-align: center;
}



.collImg{
  width: 210rpx;
  height: 210rpx;
  border-radius: 10rpx;
}
.collRight{
  margin-left: 40rpx;
  width: 60%;
}
.collName{
  margin-top: 70rpx;
  font-size:38rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color: #5D6977;
  display: block;
}
.collTime{
  color: #999;
  display: block;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
}
.shopNum{
  margin-top: 50rpx;
  display: flex;
  justify-content: space-between;
}
.delivery{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  color: #333;
}


