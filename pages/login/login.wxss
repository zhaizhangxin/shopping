/* pages/login/login.wxss */
/*授权登录  */
page{
  background: #fff;
}
.content{
  background: #fff;
  padding: 0;
  margin: 0;
}
.content::after{
  border: none;
  outline: none;
}
.cont{
  width: 100%;
  height: 600rpx;
}

.logoIcon{
  padding-top: 320rpx;
  margin: auto;
  width: 200rpx;
  height: 200rpx;
  display: block;
}
.logoText{
  padding-top: 10rpx;
  font-size:32rpx;
  font-family:MicrosoftYaHei;
  color: #333;
  display: block;
  text-align: center;
}
.conFooter{
  position: relative;
  width: 100%;
  height: 600rpx;
}
.contImg{
  width: 100%;
  height: 100%;
  display: block;
}
.conText{
  position: absolute;
  bottom: 10rpx;
  font-size:24rpx;
  font-family:MicrosoftYaHei;
  width: 100%;
  color: #666;
  display: block;
  text-align: center;
}

